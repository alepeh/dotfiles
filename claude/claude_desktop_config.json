{
  "mcpServers": {
    "mcp-obsidian": {
      "command": "/Users/alexanderpehm/.pyenv/shims/uvx",
      "args": [
        "mcp-obsidian"
      ],
      "env": {
        "OBSIDIAN_API_KEY": "${OBSIDIAN_API_KEY}",
        "OBSIDIAN_HOST": "${OBSIDIAN_HOST:-127.0.0.1}",
        "OBSIDIAN_PORT": "${OBSIDIAN_PORT:-27124}"
      }
    },
    "todoist-mcp": {
      "command": "node",
      "args": [
        "/Users/alexanderpehm/code/todoist-mcp/build/index.js"
      ],
      "env": {
        "TODOIST_API_KEY": "${TODOIST_API_KEY}"
      }
    },
    "mcp-server-github": {
      "command": "/Users/alexanderpehm/go/bin/github-mcp-server",
      "args": [
        "stdio"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    }
  },
  "globalShortcut": ""
}